{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "osml check",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "check",
                "-s",
                "./testdata/sdcard/DATA001233.DAT",
                "-o",
                "./testdata/tmp",
                "-w",
                "-r",
                "--json"
            ]
        },
        {
            "name": "osml version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "version",
                "--json"
            ]
        },
        {
            "name": "osml export",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "export",
                "-s",
                "./testdata/sdcard/DATA001233.DAT",
                "-o",
                "./testdata/tmp/",
                "-f",
                "json",
                "-n",
                "my new track",
                ""
            ]
        },
        {
            "name": "osml convert",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "convert",
                "-s",
                "./testdata/sdcard/DATA001234.DAT",
                "-f",
                "DATA001231.DAT, DATA001232.DAT",
                "-f",
                "DATA001233.DAT"
            ]
        },
        {
            "name": "osml logger",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "logger",
                "write",
                "-s",
                "./testdata/sdcard",
                "--vesselid",
                "1234",
                "-a",
                "9600",
                "-b",
                "19200",
                "--seatalk",
                "--gyro",
                "--supply",
                "--json"               
            ]
        },
        {
            "name": "osml track list",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "track",
                "list",
                "-s",
                "./testdata/sdcard/",
                "-t",
                "./testdata/tracks/track0000.zip",
                "--json"
            ],
        },
        {
            "name": "osml new track",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "track",
                "new",
                "-s",
                "./testdata/sdcard/",
                "-f",
                "DATA001231.DAT, DATA001232.DAT, DATA001233.DAT",
                "-n",
                "zürichsee 3",
                "-d",
                "description of Zürichsee",
                "-v",
                "1234",
                "-t",
                "./testdata/tracks/track0000.zip",
                "--json"
            ],
        },
        {
            "name": "osml add track",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "track",
                "add",
                "-s",
                "./testdata/sdcard/",
                "-f",
                "DATA001234.DAT, DATA001235.DAT",
                "-t",
                "./testdata/tracks/track0000.zip",
                "--json"
            ],
         },
        {
            "name": "osml convert track",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "convert",
                "-t",
                "C:/e-platte/cloudspace/nxt_willie/Software/MCSDepthLogger/Tracks/Vessel.zip",
                "--json"
            ]
         },
        {
            "name": "osml touch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/osml/main.go",
            "output" : "${workspaceFolder}/debug.exe",
            "cwd": "${workspaceFolder}",
            "args": [
                "touch",
                "-s",
                "F:/",
                "-f",
                "DATA001231.DAT, DATA001232.DAT",
                "-f",
                "DATA001233.DAT",
                "--json"
            ]
      }
    ]
}